<?xml version='1.0' encoding='utf-8'?>
<widget id="vision.irisoft.eyechart" version="1.1.52" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <name>EyeChart</name>
    <description>
        A free easy-to-use digital eye chart.
    </description>
    <author email="support@irisoft.vision" href="http://www.irisoft.vision">
        Irisoft
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
